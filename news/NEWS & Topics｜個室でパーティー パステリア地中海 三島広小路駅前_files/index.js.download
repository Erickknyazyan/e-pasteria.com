$(document).ready(function() {
	

	
	$(function(){
	var wids = $(window).width();
		if( wids > 641 ){
			$('section.read ul li:nth-child(4)').css('margin-right', '0px');
			

			// ナビ固定
			$(window).on("scroll", function () {
				// scrollTop()が0より大きい場合
				if ($(this).scrollTop() > 408) {
					$(".fixedNaviCover").slideDown();
				} else {
					$(".fixedNaviCover").fadeOut("fast");
				}
			});	
			
			if($("a[rel='colorbox']").colorbox) {
				$("a[rel='colorbox']").colorbox({fixed:'true',transition:'fade',maxWidth:'80%',maxHeight:'60%'});
			}			
			
									
			
		}else{
			$('section.read ul li:nth-child(1)').css('margin-right', '2%');
			$('section.read ul li:nth-child(3)').css('margin-right', '2%');
			$('.roomInformation .thumbnail figure:nth-child(2)').css('margin', '0px 2%');
			
			if($("a[rel='colorbox']").colorbox) {
				$("a[rel='colorbox']").colorbox({fixed:'true',transition:'fade',maxWidth:'96%',maxHeight:'90%'});
			}
			
		}
	});
	
	
	
	$(".archives dt").hover(function(){
		$(this).css("cursor","pointer"); 
	},function(){
		$(this).css("cursor","default"); 
		});
	$(".archives dd").css("display","none");
	$(".archives dt").click(function(){
		$(this).next().slideToggle("slow");
	});
		
	
	
	
	
	//Examples of how to assign the Colorbox event to elements
	$(".group1").colorbox({rel:'group1'});
	$(".group2").colorbox({rel:'group2', transition:"fade"});
	$(".group3").colorbox({rel:'group3', transition:"fade"});
	$(".group4").colorbox({rel:'group4', transition:"fade"});
	$(".group5").colorbox({rel:'group5', transition:"fade"});
	$(".group6").colorbox({rel:'group6', transition:"fade"});

	$(".ajax").colorbox();
	$(".youtube").colorbox({iframe:true, innerWidth:640, innerHeight:390});
	$(".vimeo").colorbox({iframe:true, innerWidth:500, innerHeight:409});
	$(".iframe").colorbox({iframe:true, width:"80%", height:"80%"});
	$(".inline").colorbox({inline:true, width:"50%"});
	$(".callbacks").colorbox({
		onOpen:function(){ alert('onOpen: colorbox is about to open'); },
		onLoad:function(){ alert('onLoad: colorbox has started to load the targeted content'); },
		onComplete:function(){ alert('onComplete: colorbox has displayed the loaded content'); },
		onCleanup:function(){ alert('onCleanup: colorbox has begun the close process'); },
		onClosed:function(){ alert('onClosed: colorbox has completely closed'); }
	});

	$('.non-retina').colorbox({rel:'group5', transition:'none'})
	$('.retina').colorbox({rel:'group5', transition:'none', retinaImage:true, retinaUrl:true});
	
